#include<iostream>
using namespace std;
int main(){
	int n,r;
    int rev=0;
  
    cout<<"enter a number:";
    cin>>n;
      int sum=n;
      
    while(n>0){
    	r=n%10;
    	n=n/10;
    	rev=rev*10+r;
	}
	cout<<rev<<endl;
	
	if(sum==rev){
		cout<<"it is a palindrome number";
	}
	else{
		cout<<"it is not a palindrome number";
	}
}
